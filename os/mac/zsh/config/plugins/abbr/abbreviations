abbr g="git"
abbr ga="git add"
abbr gac="git add . && git commit"
abbr gacp="git add . && git commit && git push"
abbr gc="git commit"
abbr gck="git checkout -b"
abbr ghb="gh browse"
abbr ghpr="gh pr create --fill"
abbr ghprw="gh pr create --web"
abbr ghrc="gh repo create --public --push"
abbr ghrcp="gh repo create --private --push"
abbr gi="git init"
abbr gl="git log"
abbr gp="git push"
abbr gpl="git pull"
abbr gs="git switch"
abbr gsm="git switch main"
abbr gst="git status"
